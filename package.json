{
  "name": "仓颉的早年生涯",
  "version": "0.0.1",
  "description": "a react application powerd by zola",
  "scripts": {
    "start": "ENV=dev redskull start --dashboard false",
    "test": "ENV=test redskull build",
    "build": "ENV=prod redskull build --publicPath https://fecangjie.github.io/dist/ && cp -f ./dist/index.html ./index.html && cp -f ./src/resources/images ./dist/images"
  },
  "license": "ISC",
  "dependencies": {
    "@lianjia/zola-core": "0.0.4",
    "highlight.js": "^9.12.0",
    "react-html-parser": "^1.0.3",
    "showdown": "^1.7.1",
    "summarize-markdown": "^0.3.1"
  },
  "redskull": {
    "port": 6003,
    "src": "./src",
    "entry": "index.js",
    "dist": "./dist",
    "openBrowser": false,
    "favico": "./src/favicon.ico"
  },
  "devDependencies": {
    "chokidar": "^1.7.0"
  }
}
